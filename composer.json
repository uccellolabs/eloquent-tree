{
    "name": "uccello/eloquent-tree",
    "description": "Eloquent Tree transforms a model into tree model for Laravel Eloquent ORM.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan SARDO",
            "email": "jonathan@uccellolabs.com"
        }
    ],
    "require": {
        "laravel/framework": ">=5.3"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\EloquentTree\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
